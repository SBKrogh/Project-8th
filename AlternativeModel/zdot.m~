%% Build up the f function

function [dx, y] = zdot(t, x, u, p, varargin)


pump_param;
valve_param;
elev_param; 
pipe_param;
matrices;


q = B_1'*x;

[pipe, valve, pump] = updateInputs(pipe, valve, 

alpha = pumpvec(pump, q);
lambda = - lambdavec(pipe, q);
mu = - valvevec(valve, q);

f = lambda + mu + alpha;

% for i = 1:N
%         f(i,1) = - pipe(Cp,i,B_1,z) - elev(delta_z,i,z) - valve(Cv,i,B_1,z) + pump_main(omega_main,i,flag,B_1,z) - pump_side(omega_side,i,flag,B_1,z); %Pump_side is missing !!!
% end

% for i = 1:N
%         J(i,1) = inertia(L,D,i);
% end


% Theta = (B_1*diag(J)*B_1')\B_1;

% dzdt =  f

end

